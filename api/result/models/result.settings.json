{
  "kind": "collectionType",
  "collectionName": "results",
  "info": {
    "name": "Result"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "result_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext"
    },
    "result_metrics": {
      "type": "dynamiczone",
      "components": [
        "metrics.myers-briggs"
      ],
      "max": 1
    },
    "quiz": {
      "via": "results",
      "model": "quiz"
    }
  }
}
