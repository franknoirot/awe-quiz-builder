{
  "kind": "collectionType",
  "collectionName": "results",
  "info": {
    "name": "Result"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "result_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext"
    },
    "quiz": {
      "model": "quiz",
      "via": "results"
    },
    "style_tip": {
      "type": "component",
      "repeatable": true,
      "component": "result.style-tip",
      "max": 3,
      "min": 3,
      "required": true
    },
    "theme_song": {
      "type": "string",
      "required": false
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "product_A": {
      "model": "product"
    },
    "product_B": {
      "model": "product"
    },
    "product_C": {
      "model": "product"
    },
    "product_D": {
      "model": "product"
    },
    "product_E": {
      "model": "product"
    },
    "weight": {
      "type": "decimal",
      "default": 1,
      "min": 0
    }
  }
}
