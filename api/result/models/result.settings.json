{
  "kind": "collectionType",
  "collectionName": "results",
  "info": {
    "name": "Result"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "result_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext"
    },
    "quiz": {
      "model": "quiz",
      "via": "results"
    },
    "style_tip": {
      "type": "component",
      "repeatable": true,
      "component": "result.style-tip",
      "max": 3,
      "min": 3,
      "required": true
    }
  }
}
