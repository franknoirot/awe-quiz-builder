{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "name": "Quiz"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "questions": {
      "collection": "question",
      "via": "quiz"
    },
    "results": {
      "collection": "result",
      "via": "quiz"
    },
    "interstitials": {
      "collection": "interstitial",
      "via": "quiz"
    },
    "results_metrics": {
      "type": "dynamiczone",
      "components": [
        "metrics.myers-briggs-results"
      ],
      "max": 1
    },
    "custom_styles": {
      "type": "dynamiczone",
      "components": [
        "styling.style-rule"
      ]
    }
  }
}
