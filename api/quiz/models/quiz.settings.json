{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "name": "Quiz"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "questions": {
      "collection": "question",
      "via": "quiz"
    },
    "results": {
      "collection": "result",
      "via": "quiz"
    },
    "interstitials": {
      "collection": "interstitial",
      "via": "quiz"
    },
    "results_metrics": {
      "type": "component",
      "repeatable": false,
      "component": "metrics.myers-briggs-results",
      "required": true
    },
    "custom_styles": {
      "type": "component",
      "repeatable": true,
      "component": "styling.style-rule"
    },
    "promotions": {
      "collection": "promotion"
    },
    "email_signup_content": {
      "type": "richtext"
    }
  }
}
